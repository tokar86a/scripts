	    Wait();
	    Wait(4);

		if(IsVisible(ElementByXPath("//button[@class='claim' and @disabled] ") ))
        {
           return Time_Function(); //call to function time

        } // end WaitTimerText

        var Button_Claim = ElementByXPath("//button[@class='claim' and contains(text(),'Claim')] ");  
        if (IsVisible(Button_Claim))
		{
		Click(Button_Claim);		
		}

	    Wait(5);

		Recaptcha_Function(); //call
		
		Wait(10);		
	
		if(IsVisible(ElementByXPath("//button[@class='claim' and @disabled] ") )) 
		{
		Log("FaucetClaim - Success_Claim ");  
		ClaimSuccess(); //Marks the claim as a success
		return Time_Function(); //call to function time
		}
		
      } //end FaucetClaim

        //ad = true; //call to timer / and do success	
	    Log("backup return"); 		
		return GetWaitSetting();
	

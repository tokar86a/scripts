        //=================//
       //Time_Function
        //=================//
        public int Time_Function()
        {
			
           var TimeVAR = ElementByXPath("//button[@class='claim' and @disabled] ");

           if (IsVisible(TimeVAR))
           {
	         Log("ClaimBTC - WaitTimerText apper - return to site time"); 
             int min,sec;
             min = Convert.ToInt32(TimeVAR.Text.Trim().Replace(" ", "").Split(':')[0])*60;
             sec = Convert.ToInt32(TimeVAR.Text.Trim().Replace(" ", "").Split(':')[1]);
             return min+sec;
           } 

           if (!IsVisible(TimeVAR))
           {
	            Log("ClaimBTC - return to your time"); 
		        return GetWaitSetting();
           }

           return 0;		
        }//end Time_Function
